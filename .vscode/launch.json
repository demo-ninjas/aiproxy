  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Tests",
      "type": "debugpy",
      "request": "launch",
      "cwd": "${cwd}",
      "program": "${workspaceFolder}/test/test.py",
      "console": "integratedTerminal"
    },
    {
      "name": "Debug: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    }
  ]
}
